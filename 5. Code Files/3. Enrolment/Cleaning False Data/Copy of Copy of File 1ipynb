-\n","# 2. Helper: clean text\n","# -----------------------------\n","def clean_text(x):\n","    return str(x).strip().upper()\n","\n","# -----------------------------\n","# 3. Build DISTRICT → PINCODES mapping\n","# -----------------------------\n","district_to_pins = {}\n","\n","for pin, info in pin_data.items():\n","    district = clean_text(info[\"district\"])\n","    district_to_pins.setdefault(district, []).append(int(pin))\n","\n","# -----------------------------\n","# 4. Validation + correction logic\n","# -----------------------------\n","def check_and_fix(row):\n","    df_district = clean_text(row[\"district\"])\n","    df_pin = row[\"pincode\"]\n","\n","    # District not found in JSON → cannot validate\n","    if df_district not in district_to_pins:\n","        return df_pin, \"unknown\"\n","\n","    valid_pins = district_to_pins[df_district]\n","\n","    # Pincode correct\n","    if df_pin in valid_pins:\n","        return df_pin, \"yes\"\n","\n","    # Pincode wrong → replace\n","    return valid_pins[0], \"no\"\n","\n","\n","# -----------------------------\n","# 5. Apply logic\n","# -----------------------------\n","df[[\"pincode\", \"same\"]] = df.apply(\n","    lambda r: pd.Series(check_and_fix(r)),\n","    axis=1\n",")\n","\n","# -----------------------------\n","# 6. Result\n","# -----------------------------\n","print(df.head())\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Pignvy57DzO6","executionInfo":{"status":"ok","timestamp":1768306204703,"user_tz":-330,"elapsed":23101,"user":{"displayName":"personal use","userId":"12513406510779721779"}},"outputId":"6e5c8422-4313-47b7-f50e-a12d2d113551"},"execution_count":10,"outputs":[{"output_type":"stream","name":"stdout","text":["         date         state  district  pincode  age_0_5  age_5_17  \\\n","0  27-03-2025  chhattisgarh   bijapur   494450       43        15   \n","1  27-03-2025  chhattisgarh   bijapur   494450       23        16   \n","2  01-04-2025  chhattisgarh   bijapur   494450       83        66   \n","3  01-04-2025       gujarat     surat   396510       66        25   \n","4  01-04-2025        punjab  amritsar   143102       71        23   \n","\n","   age_18_greater same  \n","0              42   no  \n","1              21   no  \n","2              66   no  \n","3              25   no  \n","4              14   no  \n"]}]},{"cell_type":"code","source":["df[df['same']=='unknown'].shape"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"FxZ2o2UTFXK0","executionInfo":{"status":"ok","timestamp":1768306224093,"user_tz":-330,"elapsed":43,"user":{"displayName":"personal use","userId":"12513406510779721779"}},"outputId":"5d29bcb1-bfa0-4802-c5d7-d43439b2830f"},"execution_count":11,"outputs":[{"output_type":"execute_result","data":{"text/plain":["(65940, 8)"]},"metadata":{},"execution_count":11}]},{"cell_type":"code","source":["df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"HcyzR0lzFX61","executionInfo":{"status":"ok","timestamp":1768306226486,"user_tz":-330,"elapsed":48,"user":{"displayName":"personal use","userId":"12513406510779721779"}},"outputId":"76c2acfd-544a-42e0-d206-3205e40e67a0"},"execution_count":12,"outputs":[{"output_type":"execute_result","data":{"text/plain":["         date         state  district  pincode  age_0_5  age_5_17  \\\n","0  27-03-2025  chhattisgarh   bijapur   494450       43        15   \n","1  27-03-2025  chhattisgarh   bijapur   494450       23        16   \n","2  01-04-2025  chhattisgarh   bijapur   494450       83        66   \n","3  01-04-2025       gujarat     surat   396510       66        25   \n","4  01-04-2025        punjab  amritsar   143102       71        23   \n","\n","   age_18_greater same  \n","0              42   no  \n","1              21   no  \n","2              66   no  \n","3              25   no  \n","4              14   no  "],"text/html":["\n","  <div id=\"df-242cb7d4-81b9-4103-afda-c72ff00cdce6\" class=\"colab-df-container