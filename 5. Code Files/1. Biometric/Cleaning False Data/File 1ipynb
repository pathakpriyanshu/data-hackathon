ion logic\n","# -----------------------------\n","def check_and_fix(row):\n","    df_district = clean_text(row[\"district\"])\n","    df_pin = row[\"pincode\"]\n","\n","    # District not found in JSON → cannot validate\n","    if df_district not in district_to_pins:\n","        return df_pin, \"unknown\"\n","\n","    valid_pins = district_to_pins[df_district]\n","\n","    # Pincode correct\n","    if df_pin in valid_pins:\n","        return df_pin, \"yes\"\n","\n","    # Pincode wrong → replace\n","    return valid_pins[0], \"no\"\n","\n","\n","# -----------------------------\n","# 5. Apply logic\n","# -----------------------------\n","df[[\"pincode\", \"same\"]] = df.apply(\n","    lambda r: pd.Series(check_and_fix(r)),\n","    axis=1\n",")\n","\n","# -----------------------------\n","# 6. Result\n","# -----------------------------\n","print(df.head())\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Pignvy57DzO6","executionInfo":{"status":"ok","timestamp":1768303871060,"user_tz":-330,"elapsed":54703,"user":{"displayName":"personal use","userId":"12513406510779721779"}},"outputId":"9e8695a1-7c75-47b9-e144-3ad09ba4e13e"},"execution_count":34,"outputs":[{"output_type":"stream","name":"stdout","text":["         date          state        district  pincode  bio_age_5_17  \\\n","0  01-03-2025  uttar pradesh      bara banki   225408            17   \n","1  01-03-2025          bihar         saharsa   852212            58   \n","2  01-03-2025          bihar           patna   803211            21   \n","3  01-03-2025    west bengal  uttar dinajpur   733252            13   \n","4  01-03-2025  uttar pradesh           unnao   209870          1265   \n","\n","   bio_age_17_     same  \n","0            3  unknown  \n","1          118       no  \n","2           20       no  \n","3           14  unknown  \n","4          369       no  \n"]}]},{"cell_type":"code","source":["df[df['same']=='unknown'].shape"],"metadata":{"colab":{"base_uri":"https://localhost:8080